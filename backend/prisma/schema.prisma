generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Cliente {
  id String @id @default(auto()) @map("_id") @db.ObjectId
  nome String
  email String
  status Boolean
  created_at DateTime? @default(now())

  @@map("clientes")
}
